{
  "extends": ["airbnb"],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "rules": {
    "max-len": ["error", { "code": 100 }],
    "react/prop-types": ["warn"]
  },
  "env": {
    "browser": true,
    "jest": true
  }
}
